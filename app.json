{
  "expo": {
    "name": "Potly",
    "slug": "Potly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "",
      "bundleIdentifier": "com.deekast.PotlyV1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "",
      "package": "com.deekast.PotlyV1"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.4",
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "scheme": "potlyv1",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "20ea25d2-1aa4-4cab-9b24-359bfc59f981"
      }
    }
  }
}
